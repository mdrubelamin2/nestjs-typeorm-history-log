  },
  "bugs": {
    "url": "https://github.com/mdrubelamin2/nestjs-typeorm-history-log/issues"
  },
  "homepage": "https://github.com/mdrubelamin2/nestjs-typeorm-history-log#readme",
  "license": "MIT",
